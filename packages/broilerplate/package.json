{
  "name": "@dr-kobros/broilerplate",
  "version": "0.21.0",
  "description": "React Reusable Broilerplate",
  "main": "lib/index.js",
  "bin": {
    "broilerplate": "./bin/broilerplate.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "NODE_ENV=production babel src -d lib",
    "prepublish": "npm run test && npm run clean && npm run build",
    "build:client": "cross-env NODE_ENV=production webpack --display-optimization-bailout --config __test-app__/webpack.config.babel.js",
    "build:empty": "cross-env NODE_ENV=production webpack --display-optimization-bailout --config __empty-app__/webpack.config.babel.js",
    "build:server": "cross-env NODE_ENV=production webpack --config __test-app__/webpack.server.config.babel.js --display-optimization-bailout",
    "build:node": "cross-env NODE_ENV=production babel __test-app__/src/index.js > __test-app__/dist-server/index.js",
    "test": "jest",
    "start:test": "cross-env NODE_ENV=development webpack-serve --config __test-app__/webpack.config.babel.js --port 9999",
    "start:empty": "cross-env NODE_ENV=development webpack-serve --config __empty-app__/webpack.config.babel.js --port 9999"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pekkis/broilerplate.git"
  },
